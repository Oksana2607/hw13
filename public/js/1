for (let i = 0; i < users.length; i++) {
            if (users[i].isActive) {
                const activeUsers = [];
                activeUsers.push(users[i]);
                usersList = activeUsers.map(user => {
                    return `<div class="users-chat__users">
                    <li class="users-chat">${user.name}</li>
                </div>`
                });
            }